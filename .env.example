# Azure Tenant Configuration
TENANT_ID=your-tenant-id
CLIENT_ID=your-api-app-client-id
CLIENT_SECRET=your-api-app-client-secret
APPLICATION_ID_URI=api://your-api-app-client-id
SPA_CLIENT_ID=your-spa-app-client-id
SCOPE_NAME=access_as_user

# Optional: Comma-separated list of allowed tenant IDs (leave empty to allow any org tenant)
ALLOWED_TENANTS=

# SharePoint Configuration
SITE_ID=your-sharepoint-site-id
LIST_ID=your-sharepoint-list-id

# Azure Document Intelligence
AZURE_DOC_INTELLIGENCE_ENDPOINT=https://your-region.cognitiveservices.azure.com
AZURE_DOC_INTELLIGENCE_KEY=your-doc-intelligence-key

# Development
SKIP_AUTH=false
NODE_ENV=development
PORT=4000
